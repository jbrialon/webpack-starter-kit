{
  "name": "webpack-starter-kit",
  "description": "ready to use webpack boilerplate",
  "private": false,
  "author": "Jeremy Brialon <jeremzor@gmail.com>",
  "main": "main.js",
  "scripts": {
    "start": "better-npm-run start",
    "build:start": "better-npm-run build:start",
    "build": "rimraf dist && better-npm-run build"
  },
  "standard": {
    "globals": [
      "__DEV__"
    ]
  },
  "betterScripts": {
    "start": {
      "command": "node build/devServer",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config build/webpack.config.prod.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "better-npm-run": "0.0.6",
    "commitizen": "^2.5.0",
    "connect-history-api-fallback": "^1.1.0",
    "conventional-changelog-cli": "^1.0.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^6.0.4",
    "html-webpack-plugin": "^2.8.1",
    "isparta": "^4.0.0",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.2.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "postcss-modules-values": "^1.1.1",
    "rb-conventional-changelog": "^1.1.9",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "standard": "^6.0.1",
    "stats-webpack-plugin": "^0.3.0",
    "style-loader": "^0.13.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
